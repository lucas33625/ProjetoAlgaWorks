create table pessoa (
    codigo bigint not null auto_increment,
    ativo bit not null,
    bairro varchar(255),
    cep varchar(255),
    cidade varchar(255),
    complemento varchar(255),
    estado varchar(255),
    logradouro varchar(255),
    numero varchar(255),
    nome varchar(255) not null,
    primary key (codigo)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;

